programa {
  funcao inicio() {
    
    Algoritmo "Multiplo6"

    //Variavel

    inteiro num

    //Inicio Algoritmo Multiplo 6

    escreva ("Informe um número ")
    leia (num)

    se (num % 6 == 0)
    escreva ( "O número ", num, " é multiplo de 6")

    senao 

    escreva ("O número ", num, " não é multiplo de 6")

    //Fim Algorimo Multiplo 6
  }
}


